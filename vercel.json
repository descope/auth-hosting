{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store, no-cache, must-revalidate, max-age=0"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "no-store"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "no-store"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/login/:path*",
			"destination": "/:path*"
		}
	]
}
